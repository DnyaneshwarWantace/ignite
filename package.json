{
  "name": "ignite",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npx dotenv-cli -e .env -- prisma db push && node scripts/dev-with-worker.js",
    "dev:next-only": "npx dotenv-cli -e .env -- prisma db push && npx dotenv-cli -e .env -- next dev",
    "dev:worker": "dotenv -c .env -- prisma db push && node scripts/dev-with-worker.js",
    "db:generate": "dotenv -c .env -- prisma db push && npx prisma generate",
    "db:push": "dotenv -c .env -- prisma db push && npx prisma db push",
    "db:studio": "dotenv -c .env -- prisma db push && npx prisma studio",
    "devbuild": "dotenv -c .env -- prisma db push && next build --no-lint",
    "devstart": "dotenv -c .env -- prisma db push && next start",
    "lint": "next lint",
    "build": "prisma generate && next build",
    "start": "dotenv -c .env.production -- next start -p 3003",
    "db:production_generate": "dotenv -c .env.production -- prisma generate",
    "db:production_push": "dotenv -c .env.production -- prisma db push",
    "db:production_studio": "dotenv -c .env.production -- npx prisma studio",
    "media-worker": "dotenv -c .env -- node scripts/media-worker.js",
    "media-retry": "dotenv -c .env -- node scripts/retry-failed.js",
    "media-status": "npx dotenv-cli -e .env -- node scripts/status.js"
  },
