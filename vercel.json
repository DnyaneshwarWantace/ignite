{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "prisma generate && next build",
  "installCommand": "npm install",
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "build": {
    "env": {
      "POSTGRES_PRISMA_URL": "@postgres_prisma_url",
      "POSTGRES_URL_NON_POOLING": "@postgres_url_non_pooling"
    }
  }
} 